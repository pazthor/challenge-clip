		{
			"name": "First endpoint - get users with payments",
			"item": [
				{
					"name": "localhost:8080/api/clip/getUsersPaymentSaved",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"It shouldn't user duplicated\",() => {",
									"    const responseJson = pm.response.json();",
									"    const result = checkIfDuplicateExists(responseJson);",
									"    pm.expect(result).to.be.false;",
									"    ",
									"   ",
									"});",
									"",
									" function checkIfDuplicateExists(arr) {",
									"        return new Set(arr).size !== arr.length",
									"    ",
									"};"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "localhost:8080/api/clip/getUsersPaymentSaved",
							"host": [
								"localhost"
							],
							"port": "8080",
							"path": [
								"api",
								"clip",
								"getUsersPaymentSaved"
							]
						}
					},
					"response": []
				}
			]
		},
